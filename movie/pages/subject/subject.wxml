<!--pages/subject/index.wxml-->
<view class="container">
  <view class="type-item" wx:for="{{types}}" wx:key="{{index}}" data-type-name="{{item.name}}" bindtap="typeMore">
    <image class="type-img" src="{{item.img}}" mode="aspectFill"></image>
    <text class="type-title">{{item.name}}</text>
  </view>
</view>